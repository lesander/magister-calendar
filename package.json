  "description": "Automatically plan your Magister appointments in your Google calendar.",
  "keywords": [
    "magister",
    "calendar",
    "planner",
    "school",
    "agenda"
  ],
  "homepage": "https://git.io/magister",
  "license": "MIT",
  "analyze": true,
  "dependencies": {
    "google-auth-library": "0.9.x",
    "googleapis": "2.1.x",
    "magister.js": "2.0.0-alpha.7",
    "npm": "^6.5.0",
    "request": "^2.88.0",
    "util": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lesander/magister-calendar.git"
